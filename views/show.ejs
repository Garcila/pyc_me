<% include partials/header %>

  <div class="main">
    <h1><%= park.name %></h1>
    <img src="<%= park.image %>" alt="" />
    <p>
      <%= park.description %>
    </p>
    <% park.comments.forEach(function(comment){ %>
    <p>
        <strong><%= comment.author %></strong> - <%= comment.text %>
    </p>
    <% }) %>
  </div>

<% include partials/footer %>
